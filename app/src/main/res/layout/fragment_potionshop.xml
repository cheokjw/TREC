<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_quiz"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/p_whitebg"
        android:layout_width="561dp"
        android:layout_height="672dp"
        android:layout_marginTop="80dp"
        android:src="@drawable/white"
        app:layout_constraintBottom_toBottomOf="@+id/p_phonecover"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/p_phonecover" />

    <ImageView
        android:id="@+id/p_mountain"
        android:layout_width="364dp"
        android:layout_height="266dp"
        android:layout_marginBottom="-80dp"
        android:src="@drawable/mountain"
        app:layout_constraintBottom_toTopOf="@+id/p_ground"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/p_ground"
        android:layout_width="348dp"
        android:layout_height="285dp"
        android:layout_marginBottom="-140dp"
        android:src="@drawable/ground"
        app:layout_constraintBottom_toBottomOf="@+id/p_phonecover"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/p_cloudone"
        android:layout_width="191dp"
        android:layout_height="121dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="-110dp"
        android:layout_marginBottom="20dp"
        android:src="@drawable/cloudone"
        app:layout_constraintBottom_toTopOf="@+id/p_mountain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/p_phonecover" />

    <ImageView
        android:id="@+id/p_cloudtwo"
        android:layout_width="183dp"
        android:layout_height="161dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="130dp"
        android:src="@drawable/cloudtwo"
        app:layout_constraintBottom_toTopOf="@+id/p_mountain"
        app:layout_constraintEnd_toStartOf="@+id/p_cloudone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/p_phonecover" />

    <ImageView
        android:id="@+id/p_tree"
        android:layout_width="198dp"
        android:layout_height="389dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/p_whitebg"
        app:srcCompat="@drawable/tree" />

    <ImageView
        android:id="@+id/p_grass"
        android:layout_width="123dp"
        android:layout_height="117dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/grass" />

    <ImageView
        android:id="@+id/potiontag"
        android:layout_width="342dp"
        android:layout_height="120dp"
        android:layout_marginTop="63dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/p_phonecover"
        app:srcCompat="@drawable/potiontag" />

    <ImageView
        android:id="@+id/p_phonecover"
        android:layout_width="488dp"
        android:layout_height="643dp"
        android:layout_marginBottom="-30dp"
        android:src="@drawable/phone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.691" />

    <ImageView
        android:id="@+id/board"
        android:layout_width="326dp"
        android:layout_height="457dp"
        android:layout_marginTop="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/p_whitebg"
        app:srcCompat="@drawable/board" />

    <ImageView
        android:id="@+id/tagspray"
        android:layout_width="117dp"
        android:layout_height="149dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/board"
        app:srcCompat="@drawable/tag" />

    <ImageView
        android:id="@+id/spray"
        android:layout_width="47dp"
        android:layout_height="132dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/tagspray"
        app:layout_constraintStart_toEndOf="@+id/tagspray"
        app:layout_constraintTop_toTopOf="@+id/tagspray"
        app:srcCompat="@drawable/spray" />

    <ImageView
        android:id="@+id/waterlevelone"
        android:layout_width="127dp"
        android:layout_height="141dp"
        android:layout_marginStart="-10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/tagwaterlevelone"
        app:layout_constraintStart_toEndOf="@+id/tagwaterlevelone"
        app:layout_constraintTop_toBottomOf="@+id/tagspray"
        app:srcCompat="@drawable/levelone" />

    <ImageView
        android:id="@+id/tagwaterlevelone"
        android:layout_width="117dp"
        android:layout_height="149dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="-35dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tagspray"
        app:srcCompat="@drawable/tag" />

    <ImageView
        android:id="@+id/bigtag"
        android:layout_width="71dp"
        android:layout_height="125dp"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tagwaterlevelone"
        app:srcCompat="@drawable/bigtag" />

    <ImageView
        android:id="@+id/waterleveltwo"
        android:layout_width="146dp"
        android:layout_height="104dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="-10dp"
        app:layout_constraintStart_toEndOf="@+id/p_grass"
        app:layout_constraintTop_toBottomOf="@+id/waterlevelone"
        app:srcCompat="@drawable/levelthree" />

    <ImageButton
        android:id="@+id/treeGrowthButton"
        android:layout_width="98dp"
        android:layout_height="101dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="88dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/p_phonecover"
        app:srcCompat="@drawable/treegrowframe" />

</androidx.constraintlayout.widget.ConstraintLayout>